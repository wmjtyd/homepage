{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "downlevelIteration": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./src",
    "paths": {
      "@/public/*": ["../public/*"],
      "@/config/*": ["config/*"],
      "@/locales/*": ["locales/*"],
      "@/stores/*": ["stores/*"],
      "@/components/*": ["components/*"],
      "@/api/*": ["api/*"],
      "@/utils/*": ["utils/*"],
      "@/micro/*": ["micro/*"],
      "@/types/*": ["types/*"],
      "@/styles/*": ["../styles/*"],
      "@/admin/*": ["admin/*"],
      "@/root/*":["../../*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "../xweb3-sdk/solana-web3.ts"],
  "exclude": ["node_modules"]
}
